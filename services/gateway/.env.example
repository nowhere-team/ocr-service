NODE_ENV=development
PORT=3000

# database
DATABASE_URL=postgresql://ocr:password@localhost:5432/ocr

# redis
REDIS_URL=redis://localhost:6379

# minio
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_USE_SSL=false
MINIO_BUCKET=images

# ocr engines
TESSERACT_URL=http://tesseract:8080
PADDLEOCR_URL=http://paddleocr:8000
ALIGNER_URL=http://aligner:8081

# thresholds
CONFIDENCE_THRESHOLD_HIGH=0.7
CONFIDENCE_THRESHOLD_LOW=0.6

# worker
WORKER_CONCURRENCY=1

# logging
LOG_LEVEL=info
LOG_FORMAT=pretty
