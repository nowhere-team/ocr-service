# server
HOST=0.0.0.0
PORT=8000
WORKERS=4

# paddle config
PADDLE_DEVICE=cpu
PADDLE_LANG=ru
PADDLE_USE_ANGLE_CLS=true
PADDLE_USE_GPU=false
PADDLE_SHOW_LOG=false

# processing
MAX_IMAGE_SIZE=10485760
PROCESSING_TIMEOUT=30.0

# logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# telemetry
ENABLE_TELEMETRY=true
OTLP_ENDPOINT=http://localhost:4317
SERVICE_NAME=paddleocr-service

# metrics
ENABLE_METRICS=true